{
  "name": "@arczinosek/polish-numeral-parser",
  "version": "0.0.3",
  "description": "Library for parsing Polish verbal notation of numbers into their digital (numerical) representation.",
  "type": "module",
  "main": "dist/polish-numeral-parser.cjs",
  "types": "dist/polish-numeral-parser.d.mts",
  "exports": {
    ".": {
      "import": "./dist/polish-numeral-parser.mjs",
      "require": "./dist/polish-numeral-parser.cjs",
      "types": "./dist/polish-numeral-parser.d.mts"
    }
  },
  "scripts": {
    "prepare": "npm run build",
    "prepublishOnly": "npm test",
    "build": "rollup -c --failAfterWarnings",
    "test": "vitest --run",
    "format": "prettier . --write",
    "format:check": "prettier . --check"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arczinosek/polish-numeral-parser-js.git"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "parser",
    "polish-number",
    "number-parser"
  ],
  "author": "Boris Grishenko <arczinosek@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/arczinosek/polish-numeral-parser-js/issues"
  },
  "homepage": "https://github.com/arczinosek/polish-numeral-parser-js#readme",
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.3.0",
    "@types/node": "^24.10.0",
    "prettier": "3.6.2",
    "rollup": "^4.52.5",
    "rollup-plugin-dts": "^6.2.3",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.7"
  }
}
